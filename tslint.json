{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "typedef": [
            true,
            "parameter",
            "property-declaration",
            "member-variable-declaration"
        ],
        "variable-name": {
            "options": [
                "check-format",
                "allow-leading-underscore",
                "ban-keywords"
            ]
        }
    },
    "rulesDirectory": []
}
